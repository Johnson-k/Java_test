package Singleton;

public class Client {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
          testSingleton01();
	}
    public static void testSingleton01(){
     	Singleton01 singleton01=Singleton01.GetInstance();
    	Singleton01 singleton02=Singleton01.GetInstance();

        System.out.println(singleton01==singleton02);
        System.out.println(singleton02);
    }
    public static void testSingleton03(){
    	Singleton03 singleton01=Singleton03.getInstance();
    	Singleton03 singleton02=Singleton03.getInstance();
        System.out.println(singleton01==singleton02);
        System.out.println(singleton02);
    }
    public static void testSingleton05(){
    	
        System.out.println(Singleton05.INSTANCE==Singleton05.INSTANCE);

    }

}
