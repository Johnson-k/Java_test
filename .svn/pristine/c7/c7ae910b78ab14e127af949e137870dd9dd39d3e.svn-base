package FlyWeight;


/**
 * 相当于外部状态，非共享的享元类
 * @author Johnsonk
 * @date   Aug 8, 2018
 */
public class Coordinate {
  private int x,y;

public Coordinate(int i, int j) {
	// TODO Auto-generated constructor stub
	x=i;
	y=j;
}

public int getX() {
	return x;
}

public void setX(int x) {
	this.x = x;
}

public int getY() {
	return y;
}

public void setY(int y) {
	this.y = y;
}
  
}
